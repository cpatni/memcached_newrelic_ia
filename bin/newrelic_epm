require File.expand_path(File.dirname(__FILE__) + "/../lib/epm")

require "new_relic/epm/cli"

status = NewRelic::EPM::CLI.execute(STDOUT, ARGV)
exit status if status
# if execute returned nil, we are running forever:
sleep
  
